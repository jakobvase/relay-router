{
  "name": "relay-router",
  "version": "0.1.3",
  "description": "A router that preloads data using relay.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "test": "test",
    "clean": "rm -r ./dist",
    "build:cjs": "tsc",
    "build:esm": "tsc --module ESNEXT --outDir ./dist/esm",
    "build:types": "tsc --outDir ./dist/types --declaration true --emitDeclarationOnly",
    "build": "yarn run clean && yarn run build:cjs && yarn run build:esm && yarn run build:types",
    "prepack": "yarn run build"
  },
  "files": [
    "./dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakobvase/relay-router.git"
  },
  "keywords": [
    "relay",
    "relayjs",
    "router"
  ],
  "author": "Jakob Vase <jakobvase@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakobvase/relay-router/issues"
  },
  "homepage": "https://github.com/jakobvase/relay-router#readme",
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "jest": "^27.5.1",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0"
  },
  "dependencies": {
    "history": "^5.2.0",
    "path-to-regexp": "^6.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
